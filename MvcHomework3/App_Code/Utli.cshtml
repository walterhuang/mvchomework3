@helper MakeLink(string url, string text)
{
    <div class="jumbotron">
        <h1>ASP.NET</h1>
        <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
        <p><a href="@url" class="btn btn-primary btn-large">@text &raquo;</a></p>
    </div>
}

@functions{
    public static string Output()
    {
        return "from functions";
    }
    
    public static IHtmlString Output2()
    {
        return new HtmlString("<h1>Output</h1>");
    }
}
